<Window x:Class="MultiSSH_Manager.ImportIP"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MultiSSH_Manager"
        mc:Ignorable="d"
        Title="Import Server" Background="#DCDCDC" Height="350" Width="330" ResizeMode="NoResize" Icon="Assets/add.ico">
    <Grid Background="#DCDCDC" Margin="1,0,0,4">
        <TextBox AcceptsReturn="True"  x:Name="List" HorizontalAlignment="Left" Margin="16,29,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="284" Height="257"/>
        <Button Content="Import" HorizontalAlignment="Left" Margin="121,277,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Height="28" FontSize="14"/>
        <Label Content="IP address per line" HorizontalAlignment="Left" Margin="93,22,0,0" VerticalAlignment="Top" FontSize="14" HorizontalContentAlignment="Stretch" Width="274" Foreground="#FFD52C2C" FontWeight="Bold"/>
        <PasswordBox x:Name="Password" HorizontalAlignment="Left" Margin="96,9,0,0" VerticalAlignment="Top" Width="204"/>
        <Label Content="Password :" HorizontalAlignment="Left" Margin="21,5,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
